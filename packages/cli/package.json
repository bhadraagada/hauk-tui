{
  "name": "@hauktui/cli",
  "version": "0.0.1",
  "description": "CLI for haukTUI - add components to your project",
  "type": "module",
  "bin": {
    "hauktui": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "keywords": ["tui", "terminal", "cli", "scaffolding"],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhadraagada/hauk-tui.git",
    "directory": "packages/cli"
  },
  "dependencies": {
    "@hauktui/registry": "workspace:*",
    "@clack/prompts": "^0.7.0",
    "commander": "^12.0.0",
    "picocolors": "^1.0.0",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  }
}
