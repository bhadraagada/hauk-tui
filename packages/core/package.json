{
  "name": "@hauktui/core",
  "version": "0.0.1",
  "description": "Core contracts, types, and utilities for haukTUI",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "keywords": ["tui", "terminal", "core", "contracts"],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhadraagada/hauk-tui.git",
    "directory": "packages/core"
  },
  "dependencies": {
    "@hauktui/tokens": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  }
}
